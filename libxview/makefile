###########################################################################
# @(#)Imakefile	1.5 24 Mar 1992 SMI
# makefile rules for Imakefile in libxview library directory.

LIBTARGET = xview
SOREV = $(SOXVIEWREV)
TARGETDIR = ./

SUBDIRS.3 =
SUBDIRS.4 = imcb

SUBDIRS= addition attr base canvas color cursor defaults dnd file_chooser filter font frame fullscreen help icon io_stream menu misc notice notify openwin panel pw rect screen scrollbar sel selection server server_image string_utils textsw ttysw win window wmgr ${SUBDIRS.3}

ALLFILES = 	Makefile.arch Makefile.master 	install_libxview.sh lorder-sparc objsort.sh version

all::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "making" all "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) "CDEBUGFLAGS=$(CDEBUGFLAGS)" all); \
	done

depend::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "depending" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS)  depend); \
	done

populate::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "populating" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS)   populate); \
	done

lintlib::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "linting" for lintlib and lintlib "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) DESTDIR=$(DESTDIR) LINTOPTS='$(LINTOPTS)' lintlib); \
	done

##########################################################################
# @(#)XView.lib	1.3 07 Jan 1992 SMI
# XView.lib for XView release 3.0.  Common commands for all library builds.

#ifdef BandAidCompiler
#include BandAidCompiler
#endif
oldinstall::
	$(MKDIRHIER) $(LIBRARY_DEST)

all:: lib${LIBTARGET}.so.${SOREV}32 lib${LIBTARGET}.so.${SOREV}64 alib64

lib${LIBTARGET}.so.${SOREV}32:: $(TEXT_OBJS) $(SPECIAL_OBJS)
	$(RM) $@~
	(cd $(SHARED_DIR)32; $(LD) -o ../$@~ $(SHLIBLDFLAGS) -m32 ?*.o)
	$(RM) $@
	$(MV) $@~ $@

lib${LIBTARGET}.so.${SOREV}64:: $(TEXT_OBJS) $(SPECIAL_OBJS)
	$(RM) $@~
	(cd $(SHARED_DIR)64; $(LD) -o ../$@~ $(SHLIBLDFLAGS) ?*.o)
	$(RM) $@
	$(MV) $@~ $@

install: lib${LIBTARGET}.so.${SOREV}32 lib${LIBTARGET}.so.${SOREV}64
	cp lib${LIBTARGET}.so.${SOREV}64 $(OPENWINHOME)/lib64/lib${LIBTARGET}.so.${SOREV}
	cp lib${LIBTARGET}.so.${SOREV}32 $(OPENWINHOME)/lib/lib${LIBTARGET}.so.${SOREV}

clean::
	$(RM) lib${LIBTARGET}.so.${SOREV} $(SHARED_DIR)/?*.o

oldinstall::
	-$(INSTALL) -c $(INSTSHAREDLIBFLAGS) lib${LIBTARGET}.sa.${SOREV} $(LIBRARY_DEST)
	-$(RANLIB) $(RANLIBINSTFLAGS) $(LIBRARY_DEST)/lib${LIBTARGET}.sa.${SOREV}

oldinstall::
	$(RM) /$(LIBRARY_DEST)/lib${LIBTARGET}.so
	$(INSTALL) -c $(INSTSHAREDLIBFLAGS) lib${LIBTARGET}.so.${SOREV} $(LIBRARY_DEST)
	(cd $(LIBRARY_DEST); $(LN) lib${LIBTARGET}.so.${SOREV} lib${LIBTARGET}.so)

oldinstall::
	-$(INSTALL) -c $(INSTLIBFLAGS) lib${LIBTARGET}.a  $(LIBRARY_DEST)
	-$(RANLIB) $(RANLIBINSTFLAGS)  $(LIBRARY_DEST)/lib${LIBTARGET}.a

lintlib:: llib-l${LIBTARGET}.ln

llib-l${LIBTARGET}.ln:
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)${LIBTARGET} $(LINTFLAGS) lint/?*.ln

clean::
	$(RM) llib-l${LIBTARGET}.ln
	$(RM) lint/?*.ln

install.ln:: lintlib

install.ln:: llib-l${LIBTARGET}.ln
	@if [ -d $(DESTDIR)$(LINT_LIBRARY_DEST) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINT_LIBRARY_DEST)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) llib-l${LIBTARGET}.ln $(DESTDIR)$(LINT_LIBRARY_DEST)/llib-l${LIBTARGET}.ln

populate::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	-@if [ -h SCCS ]; then \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	elif [ -h SCCS- ]; then \
	$(MV) SCCS- SCCS ; \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	$(MV) SCCS SCCS- ; \
	else \
	echo "SCCS or SCCS- do not exist" ; \
	fi

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
		$(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

emptyrule::

clean::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut  "#"*

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) *.[ch]

man_keywords::

# ----------------------------------------------------------------------
# rules for building in SUBDIRS - do not edit

oldinstall::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "installing" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) DESTDIR=$(DESTDIR) oldinstall); \
	done

install.man::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "installing man pages" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) DESTDIR=$(DESTDIR) install.man); \
	done

install.linkkit::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "installing link kit" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) DESTDIR='$(DESTDIR)' install.linkkit); \
	done

install_lib:
	cp libxview.* $(OPENWINHOME)/lib

install_h:
	rm -f $(XVPUB_HEADER_DEST)/*.h
	cd $(BUILDINCDIR)/$(XVPUB_INCDIR) ; cp *.h $(XVPUB_HEADER_DEST)
	chmod 444 $(XVPUB_HEADER_DEST)/*.h

# -wTRADITIONAL-CONVERSION
lib64: FORCE
	rm -f */*.o
	make lib_64

lib_64: FORCE
	OPENLOOK_BITS= make
	cp libxview.so.4.0 $(OPENWINHOME)/lib64

lib32: FORCE
	rm -f */*.o
	make lib_32

lib_32: FORCE
	OPENLOOK_BITS=-m32 make
	cp libxview.so.4.0 $(OPENWINHOME)/lib

alib64: FORCE
	rm libxview.a ; cd xshared64 ; ar r ../libxview.a *.o

FORCE:

clean::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "cleaning" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS)  clean); \
	done

tags::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo "tagging" "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS) TAGS='$(TAGS)' tags); \
	done

$(ONESUBDIR)/Makefile:
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[n]*) executeit="no";; esac; done; \
	cd $(ONESUBDIR); \
	if [ "$$executeit" != "no" ]; then \
	$(IMAKE_CMD) -DTOPDIR=$(IMAKETOP) -DCURDIR=$(ONECURDIR)$(ONESUBDIR); \
	fi;

Makefiles::
	-@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[n]*) executeit="no";; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	case "$(CURRENT_DIR)" in \
	.) curdir= ;; \
	*) curdir=$(CURRENT_DIR)/ ;; \
	esac; \
	echo "making Makefiles in $$curdir$$i..."; \
	itmp="$$i" \
	curtmp="$(CURRENT_DIR)" \
	toptmp=""; \
	case "$$itmp" in \
	../?*) \
	while echo "$$itmp" | grep '^../' > /dev/null;\
	do \
	toptmp="/`basename $$curtmp`$$toptmp"; \
	curtmp="`dirname $$curtmp`"; \
	itmp="`echo $$itmp | sed 's;../;;'`"; \
	done \
	;; \
	./?*) \
	while echo "$$itmp" | grep '^./' > /dev/null;\
	do \
	itmp="`echo $$itmp | sed 's;./;;'`"; \
	done \
	;; \
	esac; \
	case "$$itmp" in \
	*/?*/?*/?*/?*)	newtop=../../../../..;; \
	*/?*/?*/?*)	newtop=../../../..;; \
	*/?*/?*)	newtop=../../..;; \
	*/?*)		newtop=../..;; \
	*)		newtop=..;; \
	esac; \
	newtop="$$newtop$$toptmp"; \
	case "$(TOP)" in \
	/?*) imaketop=$(TOP) \
	imakeprefix= ;; \
	.) imaketop=$$newtop \
	imakeprefix=$$newtop/ ;; \
	*) imaketop=$$newtop/$(TOP) \
	imakeprefix=$$newtop/ ;; \
	esac; \
	$(RM) $$i/Makefile.bak; \
	if [ -f $$i/Makefile ]; then \
	echo "	$(MV) Makefile Makefile.bak"; \
	if [ "$$executeit" != "no" ]; then \
	$(MV) $$i/Makefile $$i/Makefile.bak; \
	fi; \
	fi; \
	$(MAKE) $(MFLAGS) $(MAKE_OPTS) ONESUBDIR=$$i ONECURDIR=$$curdir IMAKETOP=$$imaketop IMAKEPREFIX=$$imakeprefix $$i/Makefile; \
	cd $$i;	\
	$(MAKE) $(MFLAGS) Makefiles; \
	cd $$newtop; \
	done

includes::
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
	echo including "in $(CURRENT_DIR)/$$i..."; \
	(cd $$i &&  $(MAKE) $(MFLAGS)  includes); \
	done

oldall:
	rm -f */*.o
	for i in $(SUBDIRS) ; do \
	(cd $$i &&  $(MAKE) -f Makefile.orig OPENLOOK_BITS= all); \
	done
	cp libxview.so.4.0 $(OPENWINHOME)/lib64
	rm -f */*.o
	for i in $(SUBDIRS) ; do \
	(cd $$i &&  $(MAKE) -f Makefile.orig OPENLOOK_BITS=-m32 all); \
	done
	cp libxview.so.4.0 $(OPENWINHOME)/lib

